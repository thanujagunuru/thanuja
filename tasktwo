#include <iostream>
#include<cstring>
using namespace std;
bool CheckPalindrome(int i,int n,string s)
{
    if(i>=n/2)
    {
        return true;
    }
    if(s[i]!=s[n-i-1])
    {
        return false;
    }
    return CheckPalindrome(i+1,n,s);
}
int main()
{
    char s[20];
    int n;
    cin>>s;
    n=strlen(s);
    int flag=CheckPalindrome(0,n,s);
    if(flag)
     {
         cout<<"The given name is palindrome";
     }
    else
     {
         cout<<"The given name is not palindrome";
     }
    return 0;
}
